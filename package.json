{
  "name": "webpack-app",
  "version": "1.0.2",
  "description": "Webpack 4 app template",
  "scripts": {
    "build": "webpack --mode=development --progress --hide-modules",
    "production": "webpack --mode=production --progress --hide-modules --devtool=source-maps",
    "watch": "webpack --mode=development --watch",
    "start": "webpack-dev-server --open --mode=development",
    "clear": "del-cli dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manchenkoff/webpack-app.git"
  },
  "keywords": [
    "webpack",
    "manchenkoff",
    "frontend"
  ],
  "author": "Artyom Manchenkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manchenkoff/webpack-app/issues"
  },
  "homepage": "https://github.com/manchenkoff/webpack-app#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "image-webpack-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.6.0"
  },
  "dependencies": {
    "imagemin-webpack-plugin": "^2.4.2"
  }
}
